#include <stdio.h>

int main(void) {
    int number, sum = 0;
    printf("Enter integer values: \n");

    while (scanf("%d", &number) == 1) {
        sum += number;
        printf("You entered: %d\n", number);
    }
    printf("Sum of all integers %d\n", sum);
    printf("Exiting program\n");
    return 0;
}
